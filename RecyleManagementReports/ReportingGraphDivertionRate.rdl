          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="material_name">
          <DataField>material_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
