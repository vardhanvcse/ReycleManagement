  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>RecycleManagementDb</DataSourceName>
        <CommandText>(SELECT 
    convert(nvarchar,DATENAME(MONTH,collection_date)) +'-'+ convert(nvarchar,YEAR(collection_date)) AS collectiondate,
    SUM(weight_in_lbs) AS totalweight ,mat.material_name
	
FROM 
    recycling_collection rec inner join material mat on mat.material_id = rec.material_id GROUP BY 
  YEAR(collection_date), collection_date ,mat.material_name)  </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="collectiondate">
          <DataField>collectiondate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="totalweight">
          <DataField>totalweight</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="material_name">
          <DataField>material_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
